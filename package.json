{
  "name": "next-calendar",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development API_URL=http://localhost:3000 node src/core/devServer.js",
    "build": "next build",
    "start": "next start",
    "test": "jest"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^2.12.0",
    "dotenv-webpack": "^1.7.0",
    "downshift": "^5.0.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.throttle": "^4.1.1",
    "next": "^9.3.5",
    "next-offline": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-spring": "^8.0.27",
    "react-use-gesture": "^7.0.4",
    "rodal": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.3",
    "jest": "^25.1.0",
    "postcss-hexrgba": "^2.0.0",
    "postcss-media-minmax": "^4.0.0",
    "postcss-media-variables": "^2.0.1",
    "postcss-nested": "^4.2.1",
    "prettier": "^1.19.1",
    "stylelint": "^13.0.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^19.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not ie_mob 11",
    "not op_mini all",
    "not op_mob > 0",
    "not and_qq > 0",
    "not android > 0"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal"
            ]
          ]
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "aspects": [
            "invalidHref"
          ]
        }
      ],
      "default-case": "off"
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "auto"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recess-order"
    ],
    "ignoreFiles": [
      "**/*.js"
    ],
    "rules": {
      "declaration-colon-newline-after": null,
      "value-list-comma-newline-after": null,
      "no-descending-specificity": null,
      "media-feature-name-no-unknown": null
    }
  }
}
